<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="LeftButton_Visible" format="boolean">
        </attr>
        <attr name="TextView_Visible" format="boolean"></attr>
        <attr name="TextView_Name" format="string"></attr>
        <attr name="RightButton_Visible" format="boolean"></attr>
        <attr name="RightButton_Name" format="string"></attr>
    </declare-styleable>
	<declare-styleable name="OnclickBackgroud">
	    <attr name="initialize_img" format="string"/>
	</declare-styleable>
	
	<declare-styleable name="SingleLineEditProperties">
	    <attr name="backgroud_direction" format="enum">
	        <enum name="top" value="1"/>
	        <enum name="buttom" value="2"/>
	        <enum name="center" value="3"/>
	        <enum name="single" value="4"/>
	    </attr>
	    <attr name="left_textvalue" format="string"/>
	    <attr name="right_width" format="dimension"/>
	    <attr name="single_height" format="dimension"/>
	    <attr name="right_value" format="string"/>
	    <attr name="single_top" format="boolean"/>
	    <attr name="edt_line" format="integer" />
	    <attr name="edt_type" format="string"/>
	    <attr name="widget_type">
	        <enum name="datetime" value="1"/>
	        <enum name="radio" value="2"/>
	        <enum name="text" value="3"/>
	        <enum name="edit" value="4"/>
	        <enum name="editLines" value="5"/>
	        <enum name="selectList" value="6"/>
	    </attr>
	</declare-styleable>
</resources>